apiVersion: v1
kind: Secret
metadata:
  name: nextcloud-secrets
  namespace: nextcloud
type: Opaque
stringData:
  # 本番では強力なパスワードに変更してください
  NEXTCLOUD_ADMIN_USER: "admin"
  NEXTCLOUD_ADMIN_PASSWORD: "CHANGE_ME"
  POSTGRES_USER: "nextcloud"
  POSTGRES_PASSWORD: "CHANGE_ME"
  POSTGRES_DB: "nextcloud"
  REDIS_PASSWORD: "CHANGE_ME"
